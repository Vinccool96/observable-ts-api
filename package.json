{
  "name": "observable-ts-api",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./build/common/index.js",
  "module": "./build/module/index.js",
  "types": "./build/types/index.d.ts",
  "exports": {
    "import": "./build/module/index.js",
    "types": "./build/types/index.d.ts",
    "require": "./build/common/index.js"
  },
  "scripts": {
